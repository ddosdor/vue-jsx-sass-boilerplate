{
  "name": "vue-jsx-sass-boilerplate",
  "version": "0.0.1",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "start": "bnr start-dev",
    "dev": "bnr start-dev",
    "prod": "shell-exec 'bnr start-prod'",
    "build": "shell-exec 'bnr start-prod'",
    "review": "bnr start-review"
  },
  "betterScripts": {
    "start-dev": {
      "command": "webpack-dev-server --config config/webpack.config.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": "3003"
      }
    },
    "start-review": {
      "command": "webpack --config config/webpack.config.js",
      "env": {
        "NODE_ENV": "review"
      }
    },
    "start-prod": {
      "command": "webpack --config config/webpack.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/preset-env": "^7.4.1",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0-beta.3",
    "@vue/babel-preset-jsx": "^1.0.0-beta.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "better-npm-run": "^0.1.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "jest": "^24.7.1",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.0",
    "vue-style-loader": "^4.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "compression-webpack-plugin": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "string-types": "^1.0.1",
    "velocity-animate": "^1.5.2",
    "vue": "^2.6.10",
    "vue-router": "^3.0.6"
  }
}
